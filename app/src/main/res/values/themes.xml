<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Comprarei" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/darkgreen</item>
        <item name="colorPrimaryVariant">@color/darkgreen</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/darkgreen</item>
        <item name="colorSecondaryVariant">@color/darkgreen</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:actionBarStyle">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="android:actionModeBackground">@color/darkgreen</item>
        <!-- Customize your theme here. -->
    </style>
</resources>